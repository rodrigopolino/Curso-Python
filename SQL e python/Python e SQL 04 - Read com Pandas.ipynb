    "import pandas as pd\n",
    "import sqlite3\n",
    "\n",
    "conexao = sqlite3.connect(\"chinook.db\")\n",
    "\n",
    "tabela_clientes = pd.read_sql(\"SELECT * FROM customers where CustomerId = 1\", conexao)\n",
    "display(tabela_clientes)\n",
    "\n",
    "conexao.close()"
   ]
